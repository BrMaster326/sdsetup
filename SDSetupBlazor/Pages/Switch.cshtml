@page "/switch"
@inject Microsoft.AspNetCore.Blazor.Services.IUriHelper UriHelper

@functions {
	protected override async Task OnInitAsync() {
		string extension = "";
		if (UriHelper.GetAbsoluteUri().Contains("#")) extension = "#" + UriHelper.GetAbsoluteUri().Split('#').Last();
		UriHelper.NavigateTo("console?switch" + extension);
	}
}